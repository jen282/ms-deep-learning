      "source": [
        "# coding: utf-8\n",
        "import numpy as np\n",
        "import matplotlib.pylab as plt\n",
        "\n",
        "def step_function(x):\n",
        "    return np.array(x > 0, dtype=int)  # x>0이면 1을 출력\n",
        "\n",
        "X = np.arange(-5.0, 5.0, 0.1)\n",
        "y = step_function(X)\n",
        "plt.plot(X, y)\n",
        "plt.ylim(-0.1, 1.1) #y축 범위 지정\n",
        "plt.show()"
      ]
    }
